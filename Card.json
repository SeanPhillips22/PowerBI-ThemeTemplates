{
  "name": "MCPcCardTemplate",
  "visualStyles": {
    "card": {
      "*": {
        "labels": [
          {
            "color": { "solid": { "color": "#000000" } },
            "labelDisplayUnits": "0",
            "labelPrecision": 0,
            "fontSize": 19,
            "fontFamily": "Verdana"
          }
        ],
        "categoryLabels": [
          {
            "show": false,
            "color": { "solid": { "color": "#000000" } },
            "fontSize": 16,
            "fontFamily": "Verdana"
          }
        ],
        "wordWrap": [
          {
            "show": true
          }
        ],
        "background": [
          {
            "show": true,
            "color": {
              "solid": {
                "color": "#E6E6E6"
              }
            },
            "transparency": 50
          }
        ],
        "title": [
          {
            "show": true,
            "fontColor": { "solid": { "color": "#FFFFFF" } },
            "background": { "solid": { "color": "#2c3459" } },
            "alignment": "center",
            "fontSize": 13,
            "fontFamily": "Lucida Sans Unicode"
          }
        ],
        "border": [
          {
            "show": true,
            "color": { "solid": { "color": "#000000" } }
          }
        ]
      }
    }
  }
}
