{
  "name": "MCPcMatrixTemplate",
  "visualStyles": {
    "pivotTable": {
      "*": {
        "grid": [
          {
            "gridVertical": true,
            "gridVerticalColor": { "solid": { "color": "#000000" } },
            "gridVerticalWeight": 2,
            "gridHorizontal": true,
            "gridHorizontalColor": { "solid": { "color": "#000000" } },
            "gridHorizontalWeight": 2,
            "rowPadding": 2,
            "outlineColor": { "solid": { "color": "#000000" } },
            "outlineWeight": 2,
            "textSize": 8,
            "imageHeight": 75
          }
        ],
        "columnHeaders": [
          {
            "fontColor": { "solid": { "color": "#000000" } },
            "backColor": { "solid": { "color": "#FFFFFF" } },
            "outline": "Bottom",
            "autoSizeColumnWidth": true,
            "fontFamily": "Lucida Sans Unicode",
            "fontSize": 10,
            "alignment": "Center",
            "urlIcon": true,
            "wordWrap": true
          }
        ],
        "rowHeaders": [
          {
            "fontColor": { "solid": { "color": "#000000" } },
            "backColor": { "solid": { "color": "#FFFFFF" } },
            "outline": "Right",
            "stepped": true,
            "steppedLayoutIndentation": 0,
            "urlIcon": true,
            "wordWrap": false,
            "fontFamily": "Verdana",
            "fontSize": 9,
            "alignment": "Left",
            "showExpandCollapseButtons": true
          }
        ],
        "values": [
          {
            "fontColorPrimary": { "solid": { "color": "#000000" } },
            "backColorPrimary": { "solid": { "color": "FFFFFF" } },
            "fontColorSecondary": { "solid": { "color": "#000000" } },
            "backColorSecondary": { "solid": { "color": "#FFFFFF" } },
            "bandedRowHeaders": true,
            "valuesOnRow": false,
            "outline": "None",
            "urlIcon": true,
            "wordWrap": false,
            "fontFamily": "Verdana",
            "fontSize": 9
          }
        ],
        "subTotals": [
          {
            "rowSubtotals": true,
            "columnSubtotals": true,
            "fontColor": { "solid": { "color": "#000000" } },
            "fontFamily": "Verdana",
            "backColor": { "solid": { "color": "#FFFFFF" } },
            "fontSize": 9,
            "applyToHeaders": true,
            "rowSubtotalsPosition": "Top",
            "perRowLevel": true,
            "perColumnLevel": true
          }
        ],
        "total": [
          {
            "fontColor": { "solid": { "color": "#000000" } },
            "fontFamily": "Verdana",
            "backColor": { "solid": { "color": "#FFFFFF" } },
            "applyToHeaders": true,
            "fontSize": 9
          }
        ]
      }
    }
  }
}
