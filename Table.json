{
  "name": "MCPcTableTemplate",
  "visualStyles": {
    "tableEx": {
      "*": {
        "grid": [
          {
            "gridVertical": true,
            "gridVerticalColor": { "solid": { "color": "#000000" } },
            "gridVerticalWeight": 1,
            "gridHorizontal": true,
            "gridHorizontalColor": { "solid": { "color": "#FFFFFF" } },
            "gridHorizontalWeight": 2,
            "rowPadding": 2,
            "outlineColor": { "solid": { "color": "#000000" } },
            "outlineWeight": 2,
            "textSize": 8,
            "imageHeight": 75
          }
        ],
        "columnHeaders": [
          {
            "fontColor": { "solid": { "color": "#000000" } },
            "backColor": { "solid": { "color": "#FFFFFF" } },
            "outline": "Bottom",
            "autoSizeColumnWidth": true,
            "fontFamily": "Lucida Sans Unicode",
            "fontSize": 10,
            "alignment": "Center",
            "wordWrap": true
          }
        ],
        "values": [
          {
            "fontColorPrimary": { "solid": { "color": "#000000" } },
            "backColorPrimary": { "solid": { "color": "#FFFFFF" } },
            "fontColorSecondary": { "solid": { "color": "#000000" } },
            "backColorSecondary": { "solid": { "color": "#FFFFFF" } },
            "outline": "None",
            "urlIcon": true,
            "wordWrap": false,
            "fontFamily": "Verdana",
            "fontSize": 9
          }
        ],
        "total": [
          {
            "totals": true,
            "fontColor": { "solid": { "color": "#000000" } },
            "backColor": { "solid": { "color": "#FFFFFF" } },
            "outline": "Top",
            "fontFamily": "Verdana",
            "fontSize": 9
          }
        ],
        "lockAspect": [
          {
            "lockAspect": false
          }
        ]
      }
    }
  }
}
